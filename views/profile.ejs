<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="./styles/profile.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Outfit:wght@100..900&family=Rubik:ital,wght@0,300..900;1,300..900&display=swap" rel="stylesheet">
    <title>Document</title>
</head>
<body>
    <form id="bodbod">
        <div>
            <div id="my_profile1" class="my_profile">
                M Y
            </div>
    
            <div id="my_profile2" class="my_profile">

                P R O F I L E
            </div>
        </div>
        <div>
            <div class="auth_qa" id="first_auth">
                    <div class="auth_q">
                        Name
                    </div>
                    <div class="auth_a">
                        <div>
                            <input type="text" value="<%= student.name %>" readonly class="editable-input" />
                        </div>
                        <div class="image_div">
                            <image  class="edit_mark" src="./assets/edit-text 2.png" alt="its is a pencil mark">
                        </div>
                    </div>    
            </div>
            <div class="auth_qa" id="second_auth">
                <div class="auth_q">
                    Email
                </div>
                <div class="auth_a">
                    <div>
                        <%= student.email %>
                    </div>
                    <!-- <div class="image_div">
                        <img class="edit_mark" src="./assets/edit-text 2.png" alt="its is a pencil mark">
                    </div> -->
                </div>    
            </div>
            <div class="auth_qa" id="third_auth">
                <div class="auth_q">
                    Password
                </div>
                <div class="auth_a">
                    <div>
                        <input type="text" value="" readonly class="editable-input" />
                    </div>
                    <div class="image_div">
                        <img class="edit_mark" src="./assets/edit-text 2.png" alt="its is a pencil mark">
                    </div>
                </div>    
            </div>
            <div class="auth_qa" id="fourth_auth">
                <div class="auth_q">
                    Phone Number
                </div>
                <div class="auth_a">
                    <div>
                        <input type="text" value="<%= student.phone %>" readonly class="editable-input" />
                    </div>
                    <div class="image_div">
                        <img class="edit_mark" src="./assets/edit-text 2.png" alt="its is a pencil mark">
                    </div>
                </div>    
            </div>
            <div class="auth_qa" id="fifth_auth">
                <div class="auth_q">
                    Roll Number
                </div>
                <div class="auth_a">
                    <div>
                        <%= student.roll %>
                    </div>
                    <!-- <div class="image_div">
                        <img class="edit_mark" src="./assets/edit-text 2.png" alt="its is a pencil mark">
                    </div> -->
                </div>    
            </div>
            <div id="rb_row1">
                <div class="rb_r1_col" id="first_col">
                    <div class="rb_r1_q">
                        Branch
                    </div>
                    <div class="rb_r1_a">
                        <%= student.branch %>
                    </div>
                </div>
                <div class="rb_r1_col" id="second_col">
                    <div class="rb_r1_q">
                        Year
                    </div>
                    <div class="rb_r1_a">
                        UG<%= student.ug %>
                    </div>
                </div>
                <div class="rb_r1_col" id="second_col">
                    <div class="rb_r1_q">
                        Section
                    </div>
                    <div class="rb_r1_a">
                        <%= student.section %>
                    </div>
                </div>
            </div>
            <div id="rb_row2">
                <div class="rb_r2_q">
                    Courses Enrolled
                </div>
                <div id="rb_r2_course_r1">
                    <div id="course_1">
                        FFSD
                    </div>
                    <div id="course_2">
                        CCN
                    </div>
                    <div id="course_3">
                        ACS
                    </div>
                    <div id="course_4">
                        SE
                    </div>
                </div>
                <div id="rb_r2_course_r2">
                    <div id="course_5">
                        AI
                    </div>
                    <div id="course_6">
                        TOC
                    </div>
                    <div id="course_7">
                        PGP
                    </div>
                    <div id="course_8">
                        
                    </div>
                </div>
            </div>

            <div id="seperator_line"></div>
            <div id="l_row1">
                <div class="l_r1_col" id="l_r1_c1">
                    <div class="l_r1_col_q">
                        Upvotes
                    </div>
                    <div class="l_r1_col_aimg">
                        <div class="l_r1_col_a">
                            103
                        </div>
                        <div class="l_r1_col_img">
                            <img src="./assets/arrow (1) 1.png" alt="">
                        </div>
                    </div>
                </div>
                <div class="l_r1_col" id="l_r1_c2">
                    <div class="l_r1_col_q">
                        Echelons
                    </div>
                    <div class="l_r1_col_aimg">
                        <div id="echelons_a" class="l_r1_col_a">
                            72
                        </div>
                        <div class="l_r1_col_img">
                            <img src="./assets/star 1.png" alt="">
                        </div>
                    </div>
                </div>
            </div>
            <div id="l_row2">
                <div class="l_r2_col" id="l_r2_c1">
                    <div class="l_r2_col_q">
                        Total Questions Asked
                    </div>
                    <div class="l_r2_col_aimg">
                        <div class="l_r2_col_a">
                            23
                        </div>
                        
                    </div>
                </div>
                <div class="l_r2_col" id="l_r2_c2">
                    <div class="l_r2_col_q">
                        Total Questions Answered
                    </div>
                    <div class="l_r2_col_aimg">
                        <div class="l_r2_col_a">
                            19
                        </div>
                    </div>
                </div>
            </div>
            

           <img src="./assets/pfp 1.png" alt="">
